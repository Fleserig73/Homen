{
  "manifest_version": 2,
  "name": "Homen",
  "version": "1.0.0",
  "description": "make your browser more homey",
  "icons": {
    "16": "icon/icon16.png",
    "32": "icon/icon32.png",
    "48": "icon/icon48.png",
    "128": "icon/icon128.png"
  },
  "permissions": [
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}